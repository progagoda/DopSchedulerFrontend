{
  "name": "widgets",
  "version": "1.0.0",
  "description": "This library was generated with [Nx](https://nx.dev).",
  "main": "index.js",
  "scripts": {
    "visual:report": "npm-run-all visual:report:*",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui": "npx nx run widgets:build-storybook && npx loki --requireRefence --reactUri file:./storybook-static",
    "test:ui:report": "npm run visual:report:json && npm run visual:report:html",
    "approve:ui": "npx loki approve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
